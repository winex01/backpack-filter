{
    "name": "winex01/backpack-filter",
    "description": "Laravel backpack free filter without buying pro version.",
    "license": "MIT",
    "authors": [
        {
            "name": "Winnie A. Damayo",
            "email": "winnie131212592@gmail.com",
            "homepage": "https://github.com/winex01"
        }
    ],
    "homepage": "https://github.com/winex01/backpack-filter",
    "keywords": [
        "Laravel", "Backpack", "Backpack for Laravel", "Backpack Addon", "Backpack Filter"
    ],
    "require": {
        "backpack/crud": "^6.7",
        "maatwebsite/excel": "^3.1"
    },
    "require-dev": {
        "phpunit/phpunit": "~9.0",
        "orchestra/testbench": "~5|~6"
    },
    "autoload": {
        "psr-4": {
            "Winex01\\BackpackFilter\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Winex01\\BackpackFilter\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit --testdox"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Winex01\\BackpackFilter\\BackpackFilterServiceProvider"
            ]
        }
    }
}
